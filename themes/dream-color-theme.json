{
    "name": "Dream",
    "colors": {
      "editor.background": "#1c1717",
      "editor.foreground": "#F0D2D1",
      "activityBarBadge.background": "#795C5F",
      "activityBar.background": "#795C5F",
      "statusBar.background": "#795C5F",
      "sideBarTitle.foreground": "#ffffff",
      "titleBar.activeBackground": "#795C5F",
      "sideBar.background": "#272526",
      "panel.background": "#272526",
      "panel.border": "#272526",
      "button.background": "#795C5F",
      "list.activeSelectionBackground": "#795C5F",
      "textBlockQuote.background": "#F0D2D1",
      "textBlockQuote.border": "#F0D2D1",
      "textCodeBlock.background": "#F0D2D1",
      "textLink.activeForeground": "#F0D2D1",
      "textLink.foreground": "#F0D2D1",
      "textPreformat.foreground": "#F0D2D1",
      "textSeparator.foreground": "#F0D2D1"
    },
    "tokenColors": [
      {
        "name": "Comment",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#546e7a"
        }
      },
      {
        "name": "Variables",
        "scope": [
          "variable",
          "meta.definition.variable.name",
          "support.variable",
          "entity.name.variable",
          "constant.other.placeholder",
          "string constant.other.placeholder"
        ],
        "settings": {
          "foreground": "#B1DDF1"
        }
      },
      {
        "name": "Colors",
        "scope": ["constant.other.color"],
        "settings": {
          "foreground": "#ffffff"
        }
      },
      {
        "name": "Invalid",
        "scope": ["invalid", "invalid.illegal"],
        "settings": {
          "foreground": "#FF5370"
        }
      },
      {
        "name": "Keyword, Storage",
        "scope": [
          "keyword",
          "storage.type",
          "storage.modifier",
          "variable.language"
        ],
        "settings": {
          "foreground": "#cdb9fb"
        }
      },
      {
        "name": "Operator, Misc",
        "scope": [
          "keyword.control",
          "constant.other.color",
          "meta.tag",
          "punctuation.definition.tag",
          "punctuation.separator.inheritance.php",
          "punctuation.definition.tag.html",
          "punctuation.definition.tag.begin.html",
          "punctuation.definition.tag.end.html",
          "punctuation.section.embedded",
          "keyword.other.template",
          "keyword.other.substitution"
        ],
        "settings": {
          "foreground": "#8380B6"
        }
      },
      {
        "name": "Tag",
        "scope": [
          "entity.name.tag",
          "meta.tag.sgml",
          "markup.deleted.git_gutter"
        ],
        "settings": {
          "foreground": "#ee7fb8"
        }
      },
      {
        "name": "Function, Special Method",
        "scope": [
          "entity.name.function",
          "meta.function-call",
          "variable.function",
          "support.function",
          "keyword.other.special-method"
        ],
        "settings": {
          "foreground": "#CFE795"
        }
      },
      {
        "name": "Block Level Variables",
        "scope": ["meta.block variable.other"],
        "settings": {
          "foreground": "#B1DDF1"
        }
      },
      {
        "name": "Other Variable, String Link",
        "scope": ["support.other.variable", "string.other.link"],
        "settings": {
          "foreground": "#B1DDF1"
        }
      },
      {
        "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
        "scope": [
          "constant.numeric",
          "constant.language",
          "support.constant",
          "constant.character",
          "constant.escape",
          "variable.parameter",
          "keyword.other.unit",
          "keyword.other"
        ],
        "settings": {
          "foreground": "#d9aaa8"
        }
      },
      {
        "name": "String, Symbols, Inherited Class, Markup Heading",
        "scope": [
          "string",
          "punctuation",
          "constant.other.symbol",
          "constant.other.key",
          "entity.other.inherited-class",
          "markup.heading",
          "markup.inserted.git_gutter",
          "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
        ],
        "settings": {
          "foreground": "#d9aaa8"
        }
      },
      {
        "name": "Class, Support",
        "scope": [
          "entity.name",
          "support.type",
          "support.class",
          "support.other.namespace.use.php",
          "meta.use.php",
          "support.other.namespace.php",
          "markup.changed.git_gutter",
          "support.type.sys-types"
